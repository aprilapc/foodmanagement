@using _04156258.Models
@model IEnumerable<Satisfaction>
@{
    ViewBag.Title = "MealsFeedback";
    Layout = "~/Views/Shared/_RestaurantLoginLayout.cshtml";
    int id = Int32.Parse(Session["RestaurantID"].ToString());
}
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.MealsAndDiscount.MealsName)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.Score)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.MessageContent)
        </th>
        
        <th>
            @Html.DisplayNameFor(model => model.Member.MemberName)
        </th>
        
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.MealsAndDiscount.MealsName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Score)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MessageContent)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Member.MemberName)
            </td>
        </tr>
    }

</table>

<p>
    @Html.ActionLink("Back to List", "RestaurantLoginIndex",new { id = id })
</p>



